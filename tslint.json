{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-key-quotes": false,
        "object-literal-sort-keys": false,
        "no-string-literal": false,
        "ordered-imports": false,
        "variable-name": false,
        "arrow-parens": false,
        "quotemark": false,
        "semicolon": false
    },
    "rulesDirectory": []
}